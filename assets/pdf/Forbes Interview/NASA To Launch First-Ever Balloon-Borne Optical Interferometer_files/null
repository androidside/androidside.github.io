	<div id="top" class="leaderboard above_header">
		<script type='text/javascript'>
	(function() {
		var that = this,
			ad_params,
			expiration,
			render_timeout,

			getCookie = function(name) {
			    var re = new RegExp(name + "=([^;]+)"),
			    	value = re.exec(document.cookie);
			    return (value != null) ? unescape(value[1]) : null;
			  };

		try {
			ad_params = JSON.parse(getCookie('global_ad_params')) || {};
			//Only do this if they had adblock on and have turned it off. On the first ad, check to see if it returns
			if (that.adblock_state || !(ad_params && ad_params.ab && !!ad_params.ab.expiration)) {
				return;
			}
			expiration = new Date(ad_params.ab.expiration).toUTCString();
			that.adblock_state = 'checking';
			googletag.cmd.push(function() {
				googletag.pubads().addEventListener('slotRenderEnded', function(event) {
					if (that.adblock_state === 'off') {
						return;
					}
					that.adblock_state = 'off';
					clearTimeout(render_timeout);
					if ((!getCookie('welcomeAd') || !getCookie('dailyWelcomeCookie')) && !forbes.adblock_state) {
						document.cookie = "welcomeAd=true;path=/;domain=.forbes.com;expires=" + expiration;
						document.cookie = "dailyWelcomeCookie=true;path=/;domain=.forbes.com;expires=" + expiration;
					}
				});
			});
			render_timeout = setTimeout(function() {
				that.adblock_state = 'on';
				forbes.removeWelcomeCookies();

			}, 10000);
		} catch(e) {
			console.error(e);
		}
	})();
			googletag.cmd.push(function() { googletag.display('top')});
		</script>
	</div>
  <!--End: Top Ad Position-->



