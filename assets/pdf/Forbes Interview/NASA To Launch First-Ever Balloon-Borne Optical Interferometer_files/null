(function() {
	var getCookie = function(name) {
	    var re = new RegExp(name + "=([^;]+)"),
	    	value = re.exec(document.cookie);
	    return (value != null) ? unescape(value[1]) : null;
	  },
		cookie_params = {},
		now = +(new Date());

	try {
		cookie_params = JSON.parse(getCookie('global_ad_params'));
	} catch (e) {};

	for (var key in cookie_params) {
		if (!cookie_params[key] || now > (cookie_params[key].expiration || -1)) {
			delete cookie_params[key]; //Remove expired data or incomplete data
		} else {
			googletag.pubads().setTargeting(key, '' + cookie_params[key].value); //Include only value in the params object
		}
	}

	//Set the cleaned cookie for another 30 days and return clean object.
	document.cookie = "global_ad_params=" + JSON.stringify(cookie_params) + ";path=/;domain=.forbes.com;expires=" + new Date(now + 2592000000);
})();

